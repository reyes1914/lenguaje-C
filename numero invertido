// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n, resto, invertido=0;
    printf("Ingrese el numero");
    scanf("%d", &n);
    while (n !=0){
        resto = n % 10;
        n = n /10;
        invertido = invertido * 10 + resto;
    }
printf("El numero invertido es :   ");
printf("%d \n", invertido);
    return 0;
}

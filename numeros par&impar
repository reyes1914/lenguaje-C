// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;
    int valor = 0;
    do{
        printf("Ingrese un numero \n");
        scanf("%d", &n);
        if (n % 2 == 0){
            printf("%d es par \n", n);
        }else {
            printf("%d es impar \n ,n");
        }
        valor = valor +1;
        
    } while (valor < 10);
    
    return 0;
}
